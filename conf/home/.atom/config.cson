"*":
  "atom-ide-vue":
    validation: {}
  "atom-material-ui":
    colors:
      abaseColor: "#005572"
      genAccent: true
      paintCursor: true
      predefinedColor: "Blue Grey"
    fonts:
      fontSize: 15
    tabs:
      compactTabs: true
      noTabMinWidth: true
      stretchedTabs: true
      tintedTabBar: true
    ui:
      panelShadows: true
  core:
    disabledPackages: [
      "atom-ide-ui"
      "welcome"
      "metrics"
      "linter"
    ]
    telemetryConsent: "no"
    warnOnLargeFileLimit: 4
  editor:
    autoIndentOnPaste: true
    fontFamily: "Fira Code, Menlo, Consolas, DejaVu Sans Mono, monospace"
    fontSize: 15
    showIndentGuide: true
    showInvisibles: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    tabLength: 4
  "exception-reporting":
    userId: "6f8021cd-9a56-4533-bae5-7dd6c857b144"
  "file-icons":
    coloured: true
  "ftp-remote-edit":
    config: ""
    password: "4ad4494c"
    tree: {}
  "git-diff":
    showIconsInEditorGutter: true
  "ide-python":
    pylsPlugins:
      pydocstyle:
        enabled: true
    python: "python3"
  linter:
    lintOnChangeInterval: 100
  "linter-phpcs":
    autoExecutableSearch: false
    codeStandardOrConfigFile: "/opt/composer/vendor/yiisoft/yii2-coding-standards/Yii2"
    executablePath: "/opt/composer/vendor/bin/phpcs"
    ignorePatterns: [
      "**/views/**"
      "**/tests/**"
    ]
  "linter-ui-default":
    decorateOnTreeView: "Files and Directories"
    panelHeight: -41
    showPanel: true
  "markdown-preview":
    useGitHubStyle: true
  minimap:
    absoluteMode: true
    plugins:
      "minimap-autohider": true
      "minimap-autohiderDecorationsZIndex": 0
  "php-cs-fixer":
    executablePath: "/opt/composer/vendor/bin/php-cs-fixer"
    executeOnSave: true
    pathMode: "intersection"
  "php-debug":
    AutoExpandLocals: true
    currentPanelMode: "bottom"
  "php-integrator-base-legacy-php56": {}
  "spell-check":
    addKnownWords: true
    grammars: [
      "source.asciidoc"
      "source.gfm"
      "text.git-commit"
      "text.plain"
      "text.plain.null-grammar"
      "text.html"
      "text.php"
      "text.python"
      "text.js"
    ]
    localePaths: [
      "$HOME/.atom/Diccionarios"
    ]
    locales: [
      "es-ES"
      "es_ES"
      "es_ES.UTF-8"
      "en-EN"
      "en_EN"
      "en_EN.UTF-8"
      "Spanish"
      "English"
      "English (American)"
    ]
  "tree-view":
    hideIgnoredNames: true
  welcome:
    showOnStartup: false
